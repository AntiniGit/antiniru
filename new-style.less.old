.category_effect:hover figure:before,
.category_effect:hover figure:after {
  opacity: 1;
  -webkit-transform: scale(1);
  transform: scale(1);
}
.category_effect:before {
  content: '';
  background: -webkit-linear-gradient(top, transparent 0%, rgba(99, 79, 67, 0.4) 100%);
  background: linear-gradient(to top, transparent 0%, rgba(99, 79, 67, 0.4) 100%);
  width: 100%;
  height: 50%;
  opacity: 1;
  position: absolute;
  top: 0;
  left: 0;
}
